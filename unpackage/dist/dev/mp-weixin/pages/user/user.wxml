<view class="container"><block wx:if="{{isAuthInfo}}"><view class="userinfo"><view class="userinfo-con"><view class="userinfo-avatar"><image src="{{loginResult.pic?$root.g0===-1?picDomain+loginResult.pic:loginResult.pic:'../../static/images/icon/head04.png'}}"></image></view><view class="userinfo-name"><view>{{loginResult.nickName?loginResult.nickName:"用户昵称"}}</view></view></view></view></block><block wx:if="{{!isAuthInfo}}"><view class="userinfo-none"><view data-event-opts="{{[['tap',[['toLogin',['$event']]]]]}}" class="default-pic" bindtap="__e"><image src="../../static/images/icon/head04.png"></image></view><view data-event-opts="{{[['tap',[['toLogin',['$event']]]]]}}" class="none-login" bindtap="__e"><button class="unlogin">未登录</button><button class="click-login">点击登录账号</button></view></view></block><view class="list-cont"><view class="total-order"><view class="order-tit"><text style="font-weight:bold;">我的订单</text><view class="checkmore" data-sts="0" data-event-opts="{{[['tap',[['toOrderListPage',['$event']]]]]}}" bindtap="__e"><text>查看全部</text><text class="arrowhead"></text></view></view><view class="procedure"><view class="items" data-sts="1" data-event-opts="{{[['tap',[['toOrderListPage',['$event']]]]]}}" bindtap="__e"><image src="/static/images/icon/toPay.png"></image><text>待支付</text><block wx:if="{{orderAmount.unPay>0}}"><text class="num-badge">{{orderAmount.unPay}}</text></block></view><view class="items" data-sts="2" data-event-opts="{{[['tap',[['toOrderListPage',['$event']]]]]}}" bindtap="__e"><image src="/static/images/icon/toDelivery.png"></image><text>待发货</text><block wx:if="{{orderAmount.payed>0}}"><text class="num-badge">{{orderAmount.payed}}</text></block></view><view class="items" data-sts="3" data-event-opts="{{[['tap',[['toOrderListPage',['$event']]]]]}}" bindtap="__e"><image src="/static/images/icon/toTake.png"></image><text>待收货</text><block wx:if="{{orderAmount.consignment>0}}"><text class="num-badge">{{orderAmount.consignment}}</text></block>